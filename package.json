{
	"name": "@appnest/typed-actions",
	"version": "1.0.6",
	"description": "A lightweight library to create and type check flux/redux actions in typescript",
	"author": "runem",
	"license": "MIT",
	"scripts": {
		"start": "node lib/index.cjs.js",
		"watch": "rollup -c --watch",
		"build": "rollup -c",
		"checksize": "npx size-limit lib/index.esm.js"
	},
	"main": "lib/index.cjs.js",
	"module": "lib/index.esm.js",
	"typings": "lib/index.esm.d.ts",
	"files": [
		"/lib/"
	],
	"keywords": [
		"typescript",
		"typesafe",
		"actions",
		"redux",
		"flux",
		"action-creator"
	],
	"dependencies": {},
	"devDependencies": {
		"@wessberg/rollup-plugin-ts": "^1.1.31",
		"rollup": "^1.1.2",
		"typescript": "^3.3.1"
	}
}
